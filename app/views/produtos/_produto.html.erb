<div id="<%= dom_id produto %>">
  <p>
    <strong>Name:</strong>
    <%= produto.name %>
  </p>
  <p>
    <strong>Foto:</strong>
    <img src="<%= produto.foto %>" class="produto-thumbnail" alt="<%= produto.name %>">
  </p>

  <p>
    <strong>Preco:</strong>
    <%= produto.preco %>
  </p>

  <p>
    <strong>Descricao:</strong>
    <%= produto.descricao %>
  </p>

</div>
